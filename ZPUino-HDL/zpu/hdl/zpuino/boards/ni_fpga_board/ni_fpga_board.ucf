##################################################################################
## ni_fpga_board.ucf
##
## Author: Ivan Deras
##
## Contains assignment and iostandard information for
## all used pins as well as timing and area constraints for 
## National Instruments FPGA Board that uses a 50Mhz oscillator.
##
##################################################################################

# Crystal Clock - use 50MHz onboard oscillator
NET "clk"  LOC="B8" | IOSTANDARD = LVCMOS33 | PERIOD = 20ns ;

# J1
Net "J1<0>" LOC ="C15" | IOSTANDARD = LVCMOS33 | SLEW = FAST | DRIVE = 8;
Net "J1<1>" LOC ="F13" | IOSTANDARD = LVCMOS33 | SLEW = FAST | DRIVE = 8;
Net "J1<2>" LOC ="F14" | IOSTANDARD = LVCMOS33 | SLEW = FAST | DRIVE = 8;
Net "J1<3>" LOC ="G13" | IOSTANDARD = LVCMOS33 | SLEW = FAST | DRIVE = 8;
Net "J1<4>" LOC ="F12" | IOSTANDARD = LVCMOS33 | SLEW = FAST | DRIVE = 8;
Net "J1<5>" LOC ="D14" | IOSTANDARD = LVCMOS33 | SLEW = FAST | DRIVE = 8;
Net "J1<6>" LOC ="F15" | IOSTANDARD = LVCMOS33 | SLEW = FAST | DRIVE = 8;
Net "J1<7>" LOC ="G14" | IOSTANDARD = LVCMOS33 | SLEW = FAST | DRIVE = 8;

#J2
