}

void DrawLives()
{
  int pos =6;
  for(int i=0;i<Lives;i++)
  {
    if(HeartAnimation==0)
      VGA.writeArea(pos,110 , 9, 8, Heart);
    else
      VGA.writeArea(pos,110 , 9, 8, Heart2);
    pos+=10;
    HeartAnimation++;
    if(HeartAnimation>1)
      HeartAnimation=0;
  }
  
}

