  PersonAnimation++;
  
  if(PersonAnimation>1)
    PersonAnimation=0;
};

void DrawCar()
{
    if(CarAnimation==0)
      VGA.writeArea(83, 85, 20, 29, Car);
    else
      VGA.writeArea(83, 86, 20, 29, Car);
    
    CarAnimation++;
    if(CarAnimation>1)
      CarAnimation=0;
}

